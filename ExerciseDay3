Create a list(ArrayList) of Employees. Add at least 5 employees to the list. Print the list of employees every time a new employee is added.
Employee is a class{empId, empName, empSal} + both constructors + getters and setters
List<Employee> emps â€¦
Hint: override toString() in Employees


package Day3;



import java.util.ArrayList;



public class Employee{

	

	public static ArrayList<Employee> EmpList = new ArrayList<>();

	

	int empId;

	String empName;

	double empSal;

	

	public Employee(int empId, String empName,double empSal){

		this.empId=empId;

		this.empName=empName;

		this.empSal=empSal;

		for(Employee e:EmpList) {

			System.out.println(e);

		}

	}

	

	public Employee() {

		for(Employee e:EmpList) {

			System.out.println(e);

		}

	}

	

	public int EmpId() {

		return empId;

	}

	public String EmpName() {

		return empName;

	}

	public double EmpSal() {

		return empSal;

	}

	

	@Override

	public String toString() {

		return (empId+" "+empName+" "+empSal);

	}

	

	public static void main(String[] args) {

		

		EmpList.add(new Employee(1,"Vibhuti",30000));

		EmpList.add(new Employee(2,"Rohit",40000));

		EmpList.add(new Employee(3,"Sam",20000));

		EmpList.add(new Employee(4,"Pankaj",50000));

		EmpList.add(new Employee(5,"Rushikesh",60000));

	}

}










package Day3;



import java.util.ArrayList;



public class Employee{

	

	public static ArrayList<Employee> EmpList = new ArrayList<>();

	

	int empId;

	String empName;

	double empSal;

	

	public Employee(int empId, String empName,double empSal){

		this.empId=empId;

		this.empName=empName;

		this.empSal=empSal;

		for(Employee e:EmpList) {

			System.out.println(e);

		}

	}

	

	public static void Add(int empId, String empName,double empSal) {

		EmpList.add(new Employee(empId,empName,empSal));

	}

	

	public Employee() {

		for(Employee e:EmpList) {

			System.out.println(e);

		}

	}

	

	public int EmpId() {

		return empId;

	}

	public String EmpName() {

		return empName;

	}

	public double EmpSal() {

		return empSal;

	}

	

	@Override

	public String toString() {

		return (empId+" "+empName+" "+empSal);

	}

	

	public static void main(String[] args) {

		

		Add(1,"Vibhuti",30000);

		Add(2,"Rohit",40000);

		Add(3,"Sam",20000);

		Add(4,"Pankaj",50000);

		Add(5,"Rushikesh",60000);

	}

}















